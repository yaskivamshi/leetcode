class Solution:
    def countPairs(self, nums: List[int], target: int) -> int:
        n=len(nums)
        nums.sort()
        no_pairs = 0
        l=0
        r=n-1
        while l<r:
            if nums[l]+nums[r]<target:
                no_pairs+=r-l
                l+=1
            else:
                r-=1
        return no_pairs
        
        
